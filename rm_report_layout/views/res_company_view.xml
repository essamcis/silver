<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="view_company_form_report_custom_inherit" model="ir.ui.view">
        <field name="name">company.form.report.custom.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">



            <xpath expr="//notebook" position="inside">
                <page name="document_setting" string="Document Setting">
                    <group>
                        <field name="header_image" widget="image" />
                        <field name="footer_image" widget="image" />
                    </group>
                </page>
            </xpath>




        </field>
    </record>


