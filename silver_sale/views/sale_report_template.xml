<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_saleorder_document_inherit_silver"
              inherit_id="sale.report_saleorder_document">


        <xpath expr="//table[hasclass('o_main_table')]/thead/tr/th[1]"
               position="after">

            <th>Brand</th>

        </xpath>
        <xpath expr="//table/tbody[hasclass('sale_tbody')]/t[2]/tr/t[1]/td[1]"
               position="after">

                <td>
                    <span t-field="line.product_brand_id.name"/>
                </td>
        </xpath>
    </template>

</odoo>
